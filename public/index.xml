    grid-column: 1 / -1;
    }

    .fs-fieldset-title {
    color: var(--color-text-header);
    font-family: var(--font-family-display);
    font-size: 1.25rem;
    line-height: 1.75rem;
    margin-bottom: 1.5rem;
    grid-column: 1 / -1;
    }

    .fs-field {
    display: flex;
    flex-direction: column;
    row-gap: 0.5rem;
    }

    .fs-label {
    color: var(--color-text-default);
    display: block;
    font-family: var(--font-family-display);
    font-size: 1rem;
    line-height: 1.25rem;
    }

    .fs-description {
    color: var(--color-text-header);
    display: block;
    font-size: 1rem;
